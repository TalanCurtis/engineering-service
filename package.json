{
  "name": "icon-editor",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3010",
  "main": "./server/server.js",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "1.18.3",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "gsap": "2.0.2",
    "lodash": "^4.17.15",
    "massive": "^5.11.2",
    "node-sass": "4.9.3",
    "node-sass-chokidar": "1.3.3",
    "npm-run-all": "4.1.3",
    "passport": "0.4.0",
    "passport-auth0": "1.0.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-scripts": "^3.0.1",
    "redux": "4.0.0",
    "redux-promise-middleware": "5.1.1",
    "sass": "1.13.0",
    "sass-loader": "7.1.0"
  },
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
